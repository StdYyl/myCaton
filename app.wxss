view, scroll-view, swiper, swiper-item, cover-view, cover-image, icon, text,
checkbox, form, input, label, picker, picker-view, button, checkbox-group,
radio-group, radio, slider, switch, textarea, navigator, map, canvas,  {
  box-sizing: border-box;
}

page {
  background: #fff;
}

.list-masonry {
  column-count: 2;
  column-gap: 17rpx;
  -moz-column-count: 2;
  -webkit-column-count: 2;
}

.nav-line:before {
  content: " ";
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  color: #ccc;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 100;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.weui-cell:before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  left: 15px;
  z-index: 2;
}

.weui-cells__title {
  margin-top: 0.77em;
  margin-bottom: 0.3em;
  padding-left: 30rpx;
  padding-right: 30rpx;
  color: #999;
  font-size: 28rpx;
}

.weui-cells {
  margin-top: 0;
  background-color: #fff;
  line-height: 1.47058824;
  font-size: 34rpx;
  overflow: hidden;
  position: relative;
}

.weui-cells:after, .weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: #e5e5e5;
  z-index: 2;
}

.weui-cells:before {
  top: 0;
  border-top: 1px solid #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.weui-cells:after {
  bottom: 0;
  border-bottom: 1px solid #e5e5e5;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.weui-cell {
  padding: 20rpx 30rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 32rpx;
}

.weui-cell_access {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  color: inherit;
}

.weui-label {
  display: block;
  width: 210rpx;
  word-wrap: break-word; /*在长单词或 URL 地址内部进行换行*/
  word-break: break-all; /*应用于除了汉字外点等一系列的多行显示问题*/
}

.weui-cell__bd {
  flex: 1;
}

.weui-input {
  width: 100%;
  border: 0;
  outline: 0;
  -webkit-appearance: none; /*移除原生控件样式*/
  background-color: transparent; /*背景透明，一般不用*/
  font-size: inherit; /*继承父类*/
  color: inherit; /*继承父类*/
  height: 1.47058824em;
  line-height: 1.47058824;
  user-select: auto; /*根据浏览器自动选择是否可以选中*/
  font-family: inherit;
}

.weui-cells_form input, .weui-cells_form label[for], .weui-cells_form textarea {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0); /*禁止苹果手机的点击后高亮闪动*/
}

.weui-textarea {
  display: block;
  border: 0;
  resize: none; /*去掉右下角图标*/
  width: 100%;
  color: inherit;
  font-size: 1em; /*？？？为什么要用em*/
  line-height: inherit;
  outline: 0;
}

.weui-cell__ft {
  margin-top: 4rpx;
}

.weui-cell__ft_in-access {
  padding-right: 28rpx;
  position: relative;
}

.weui-cell__ft_in-access:after {
  content: " ";
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: rgba(187, 187, 187, 1);
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 4rpx;
}

.fix-bar {
  left: 0;
  right: 0;
  margin: 0 auto;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  color: #333;
  font-size: 28rpx;
  position: fixed;
  z-index: 99;
  bottom: 0;
}

.weui-navbar__slider {
  position: absolute;
  content: " ";
  left: 0;
  bottom: 0;
  width: 50%;
  height: 4rpx;
  background-color: #3E84FF;
  -webkit-transition: -webkit-transform 0.3s;
  transition: -webkit-transform 0.3s;
  transition: transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
}

.fix-iphonex-button {
  padding-bottom: constant(safe-area-inset-bottom); /*适配低高版本的ios*/
  padding-bottom: env(safe-area-inset-bottom); /*适配低高版本的ios*/
  padding-bottom: 68rpx !important;
}

.weui-cell_access:active {
  background-color: #ececec;
}

.fix-bar:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e5e5e5;
  color: #e5e5e5;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
