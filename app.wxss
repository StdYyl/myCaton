.animate-wave {
  animation: cloud 10s linear infinite;
  -webkit-animation: cloud 10s linear infinite;
  -o-animation: cloud 10s linear infinite;
}

@keyframes cloud {
  from {
    background-position-x: 0%;
    background-position-y: 0%;
  }

  to {
    background-position-x: 750rpx;
    background-position-y: 0%;
  }
}

