@keyframes narrowIn {
  from {
    height: 0rpx;
    opacity: 0;
  }

  to {
    height: 100%;
    opacity: 1;
  }
}

.view-narrow-in {
  animation: narrowIn ease 0.5s forwards;
}

@keyframes narrowOut {
  from {
    height: 100%;
    opacity: 1;
  }

  to {
    height: 0rpx;
    opacity: 0;
  }
}

.view-narrow-out {
  animation: narrowOut ease 0.5s forwards;
}

@keyframes arrowIn {
  from {
    transform: rotate(-45deg);
    bottom: -5rpx;
  }

  to {
    transform: rotate(45deg);
    bottom: 0rpx;
  }
}

.view-arrow-in {
  animation: arrowIn ease 0.5s forwards;
}

@keyframes arrowOut {
  from {
    transform: rotate(45deg);
    bottom: 0rpx;
  }

  to {
    transform: rotate(-45deg);
    bottom: -5rpx;
  }
}

.view-arrow-out {
  animation: arrowOut ease 0.5s forwards;
}

